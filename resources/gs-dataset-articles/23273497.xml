            <ArticleTitle>MR features of inflammatory hepatic adenoma.</ArticleTitle>
            <Pagination>
                <MedlinePgn>109-10</MedlinePgn>
            </Pagination>
            <ELocationID EIdType="doi" ValidYN="Y">10.1016/j.clinre.2012.10.008</ELocationID>
